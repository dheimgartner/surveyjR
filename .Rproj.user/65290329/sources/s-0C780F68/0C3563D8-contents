#' Register internal JavaScript functions
#'
#' @return `shinyjs::extendShinyjs()`
#' @export
registerJs <- function() {
  
  if (!("shinyjs" %in% names(resourcePaths()))) {
    stop("Register shinyjs::useShinyjs() in your UI")
  }
  
  shinyjs::extendShinyjs(script = "js/renderSurvey.js", functions = c("renderSurvey"))
}